<?xml version="1.0" encoding="utf-8"?>
<uiManifest>
  <!--VM Backup-->
  <extension name="VmBackupTenantExtension"
             baseUri="~/Content/VmBackupTenant">
    <scripts>
      <script src="~/Scripts/ViewModel/HW.ViewModels.QuickCreate.js" />
      <script src="~/Scripts/VmBackupTenant.Controller.js" />
      <script src="~/Scripts/VmBackupTenant.VmBackupTab.js" />
      <script src="~/Scripts/VmBackupTenant.SettingsTab.js" />
      <script src="~/VmBackupTenantExtension.js" />

      <!--Self registration-->
      <script src="~/extensions.data.js" />
    </scripts>
    <stylesheets>
      <stylesheet src="~/Styles/VmBackupTenant.css"/>
    </stylesheets>
    <templates>
      <template name="VmBackupTab"
                src="~/Templates/Tabs/VmBackupTab.html" />
      <template name="VmBackupTabEmpty"
                src="~/Templates/Tabs/VmBackupTabEmpty.html" />
      <template name="selectRestorePointTemplate"
                src="~/Templates/Dialogs/VmBackupSelectRestorePointWizard.html" />
      <template name="CreateFileShare"
                src="~/Templates/Dialogs/CreateFileShare.html" />
      <template name="settingsTab"
                src="~/Templates/Tabs/SettingsTab.html" />
    <template name="CreateFileShare"
                src="~/Templates/Menu/FileShareQuickCreate.html" />
    </templates>
  </extension>
</uiManifest>
